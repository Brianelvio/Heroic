{
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 Chrome versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "babel-plugin-root-import"
    ],
    "ignore": [
      "/public/**/*",
      "node_modules/**/*"
    ],
    "compact": true
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.4",
    "fastify": "^1.9.0",
    "fastify-formbody": "^2.0.1",
    "fastify-helmet": "^1.0.3",
    "fastify-static": "^0.12.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.4.0",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "mysql2": "^1.5.3",
    "objection": "^1.2.0",
    "objection-password-bcryptjs": "^1.0.0-a",
    "objection-visibility": "^0.3.2",
    "randomstring": "^1.1.5",
    "request-ip": "^2.1.1",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1"
  },
  "scripts": {
    "serve": "nodemon -L --exec babel-node index.js"
  },
  "standard": {
    "ignore": [
      "/public",
      "/node_modules"
    ]
  }
}
